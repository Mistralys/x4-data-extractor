{
  "name": "mistralys/x4-data-extractor",
  "description": "Generate batch files to extract X4 data with XRCatTool.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Mordziol",
      "email": "s.mordziol@mistralys.eu"
    }
  ],
  "autoload": {
    "classmap": [
      "src"
    ]
  },
  "require": {
    "mistralys/application-utils-core": ">=2.3.11",
    "mistralys/application-utils-collections": ">=1.1.6",
    "php": ">=8.2",
    "ext-dom": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9.5.20",
    "phpstan/phpstan": ">=1.6.1",
    "roave/security-advisories": "dev-latest"
  },
  "scripts": {
    "build-batches": "Mistralys\\X4\\DataExtractor\\BuildTools::build",
    "list-folders": "Mistralys\\X4\\DataExtractor\\BuildTools::listDataFolders"
  }
}
